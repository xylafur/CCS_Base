<bitenum id="ON" value="1" token="ON" description="AR1 is used for circular addressing."/>
</bitfield>
<bitfield id="AR0LC" width="1" begin="0" end="0" resetval="0" description="AR0 linear/circular configuration bit." range="" rwaccess="RW">
